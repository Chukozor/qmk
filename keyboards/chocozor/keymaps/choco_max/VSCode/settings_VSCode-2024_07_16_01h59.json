{
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.mouseWheelScrollSensitivity": 5,
  "editor.fontSize": 16,
  "git.autofetch": true,
  "files.associations": {
    "*.h": "c",
    "*.gcode": "gcode"
  },
